<%include ../shared/header2%>
<div class="container eric-container-all " style="text-align: center;">

    <div class="eric-contanct-body animated bounceInLeft ">
        <h1 class="eric-h1-reg">Manufacturers</h1>
        <%if(err){%>
        <h3><%=err%></h3>
        <%}%>
<form method="post" >
    <fieldset class="form-group">
        <label class="control-label col-md-2" for="name">Name: *</label>
        <input name="name" id="name" required/>
    </fieldset>
    <fieldset class="form-group">
        <label class="control-label col-md-2" for="model">Country: *</label>
        <input name="country" id="country" required />
    </fieldset>
    <fieldset class="form-group">
        <label class="control-label col-md-2"for="year">Year: *</label>
        <input name="year" id="year" required />
    </fieldset>

    <button class="btn eric-normal-btn">Save</button>
</form>
    </div>
</div>
<%include ../shared/footer%>